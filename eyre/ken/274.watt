::::
::
    byte  &@
    tape  (list byte)
    tube  &[p=*hair q=*tape]
    edge  &[p=*hair q=*(unit &[p=* q=*tube])]
    hair  &[p=@ q=@]
    rule  =+(tub=*tube |=(*edge))
    spew  =+(* |:([p=*hair q=?:(.=(-< -<) ~ [~ u=[p=-< q=*tube]])]))
  ::::
  ::
      bass
    =+  [rum=@ tyd=rule]
    |:
    %+  cook
      =+(*(list &@) |=((roll -< =+([a=@ b=@] |=((add a (mul rum b)))))))
    tyd
  ::::
  ::
      boss
    =+  [rum=@ tyd=rule]
    |:
    %+  cook
      =+(*(list &@) |=((reel -< =+([a=@ b=@] |=((add a (mul rum b)))))))
    tyd
  ::::
  ::
      bend
    =+  raq==+([* *] |:(-<))
    |:
    =+  [vex=*edge sab=rule]
    |:
    ?~  q.vex
      vex 
    =+  yit=(sab q.u.q.vex)
    ?~  q.yit
      [p=p.yit q=q.vex]
    [p=p.yit q=[~ u=[p=(raq p.u.q.vex p.u.q.yit) q=q.yit]]]
  ::::
  ::
      cold
    =+  [cus=* sef=rule]
    |:
    =+  tub=*tube
    |=
    =+  vex=(sef tub)
    ?~  q.vex
      vex
    [p=p.vex q=[~ u=[p=cus q=q.u.q.vex]]]
  ::::
  ::
      cook
    =+  [poq=&* sef=rule]
    |:
    =+  tub=*tube
    |=
    =+  vex=(sef tub)
    ?~  q.vex
      vex
    [p=p.vex q=[~ u=[p=(poq p.u.q.vex) q=q.u.q.vex]]]
  ::::
  ::
      comp
    =+  raq==+([* *] |:(-<))
    |:
    =+  [vex=*edge sab=rule]
    |:
    ?~  q.vex
      vex
    =+  yit=(sab q.u.q.vex)
    ?~  q.yit
      yit
    [p=p.yit q=[~ u=[p=(raq p.u.q.vex p.u.q.yit) q=q.yit]]]
  ::::
  ::
      fail
    =+  tub=*tube
    |=
    [p=p.tub q=~]
  ::::
  ::
      fine
    =+  vex=*edge
    |:
    ?~  q.vex
      ^?([%chug %flu p.vex] !!)
    ?:  .=(~ q.u.q.vex)
      p.u.q.vex
    ^?([%chug %gas p.u.q.vex] !!)
  ::::
  ::
