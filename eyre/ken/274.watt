    =+  huf=*
    |:
    =+  tub=*tube
    |=
    [p=p.tub q=[~ u=[p=huf q=tub]]]
  ::::
  ::
      ifix
    =+  [fel=[p=rule q=rule] hof=rule]
    |:
    |~(pfix p.fel |~(sfix hof q.fel))
  ::::
  ::
      just
    =+  daf=*byte
    |=
    =+  tub=*tube
    |=
    ^-  (spew *byte)
    ?~  q.tub
      (fail tub) 
    ?.  .=(daf i.q.tub)
      (fail tub)
    =+  suz=(nair i.q.tub p.tub)
    [suz [~ daf [suz t.q.tub]]]
  ::::
  ::
      mask
    =+  bud=*(list byte)
    |=
    =+  tub=*tube
    |=
    ^-  (spew *byte)
    ?~  q.tub
      (fail tub) 
    ?.  (lien bud =+(*byte |=(.=(i.q.tub -<))))
      (fail tub)
    =+  suz=(nair i.q.tub p.tub)
    [suz [~ i.q.tub [suz t.q.tub]]]
  ::::
  ::
      more
    =+  [bus=rule fel=rule]
    |:
    |~  pose
      |~(plug fel (star |~(pfix bus fel)))
      (free ~)
    ==
  ::::
  ::
      nair
    =+  [weq=*byte naz=*hair]
    |=
    ^-  *hair
    ?:  .=(10 weq)
      [.^(p.naz) 0]
    [p.naz .^(q.naz)]
  ::::
  ::
      most
    =+  [bus=rule fel=rule]
    |:
    |~(plug fel (star |~(pfix bus fel)))
  ::::
  ::
      pfix
    =+  [vex=*edge sab=rule]
    |:
    ?~  q.vex
